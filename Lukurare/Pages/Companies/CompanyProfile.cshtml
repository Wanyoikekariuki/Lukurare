                                                            <label for="history">History and Background</label>
                                                            <textarea class="form-control" id="history" rows="3" placeholder="Enter your History and Background" data-bind="value: history"></textarea>
                                                        </div>
                                                        <div id="alertArea" class="alert" role="alert" style="display: none;"></div>
                                                        <button type="submit" class="btn btn-primary" data-bind="click: companyOverview">Submit</button>
                                                    </form>
                                                </div>
