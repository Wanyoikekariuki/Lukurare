@page
@model SeekersHub.Pages.Companies.CompanyProfileModel
@{
    Layout = "/Pages/Shared/_CompaniesLayout.cshtml";
    ViewData["Title"] = "Company Profile";
}

<main class="main">
    <section class="section-box-2">
        <div class="container">
            <div class="banner-hero banner-image-single"><img src="/assets/imgs/page/candidates/img.png" alt="SeekersHub"><a class="btn-editor" href="#"></a></div>
            <div class="box-company-profile">
                <div class="image-compay"><img data-bind="attr: { src: PhotoLinkUrl}" alt="SeekersHub" style="width: 100px; height: 100px;"></div>
                <div class="row mt-10">
                    <div class="col-lg-8 col-md-12">
                        <h5 class="f-18" data-bind="text: Brand"><span class="card-location font-regular ml-20" data-bind="text: Location"></span></h5>
                        @*<p class="mt-0 font-md color-text-paragraph-2 mb-15">UI/UX Designer. Front end Developer</p>*@
                    </div>
                    @*<div class="col-lg-4 col-md-12 text-lg-end"><a class='btn btn-preview-icon btn-apply btn-apply-big' href='page-contact.html'>Preview</a></div>*@
                </div>
            </div>
            <div class="border-bottom pt-10 pb-10"></div>
        </div>
