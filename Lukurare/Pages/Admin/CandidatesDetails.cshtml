                                    <div class="modal-dialog">
                                        <div class="modal-content">
                                            <div class="modal-header">
                                                <h5 class="modal-title" id="matchCandidateModalLabel">Match Candidate</h5>
                                                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                            </div>
                                            <div class="modal-body">
                                                <!-- Add your form here -->
                                                <form>
                                                    <div class="mb-3">
                                                        <label for="companySelect" class="form-label">Select Company</label>
                                                        <select class="form-select" data-bind="options: Companies,
                                        optionsCaption: 'Choose...',
                                        optionsText: function(item) {
                                            return item.EntityName + ' - ' + item.Email;
                                        },
                                        value: selectedCompany">
                                                        </select>
                                                    </div>
                                                    <div id="alertArea" class="alert" role="alert" style="display: none;"></div>
                                                </form>
                                            </div>
                                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                                            <button type="button" data-bind="click: function(data, event) { $root.matchCandidate(data, event) }" class="btn btn-primary">Submit</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
